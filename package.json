{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "build:svg": "svgr ./ressources/**/icons/ -d ./src/atoms/icons --typescript",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-build",
    "build-storybook-with-assets": "build-storybook -o docs-build -s ./stories/assets"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "storybook-library",
  "author": "Cyril Hue",
  "module": "dist/storybook-library.esm.js",
  "size-limit": [
    {
      "path": "dist/storybook-library.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/storybook-library.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@size-limit/preset-small-lib": "^7.0.5",
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.1",
    "babel-loader": "^8.2.3",
    "husky": "^7.0.4",
    "postcss": "^8.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^7.0.5",
    "tailwindcss": "^3.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@heroicons/react": "^1.0.5",
    "@svgr/cli": "^6.1.2",
    "@tailwindcss/forms": "^0.4.0",
    "@types/tailwindcss": "^3.0.2",
    "postcss-cli": "^9.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "tailwind-scrollbar": "^1.3.1",
    "tailwind-scrollbar-hide": "^1.1.7"
  }
}
